[dock]
# defines the icon size. min value is 24 max value 128
# default is 52 pixels.
icon_size = 52

# define the style to use. If not set the default style will be use. 
#style = style.debian
#style = style.gray
style = style.heater
#tyle = style.maco
#style = style.black
#style = style.duque
#style = style.debian


preview_image_size = 260


monitor_name =

# the dock location settings are top, right, bottom or left.
# command lime parameter --l right
# default bottom  
location = 

# the dock alignment settings are start, end, center or fill.
# command line parameter --r fill
# default center
alignment = center

# the dock autohide settings are none, autohide or intelihide
# default intelihide
autohide_mode = none

# the anchor margin in pixel to make a hide dock show up when the mouse is over the monitor rand.
# the default value is 20 pixels. Posible value are 1 to 20.
anchor_margin = 2

# the easing animation delay time when the dock show or hide
# the default value is 5.0 frames.
# The valid range of values are from 0.0 to 20.0 frames 
animation_delay = 4.0

# the duration in seconds before the dock start hiding the dock.
# the default value is 0.5 seconds.
# posible values are from 0.0 to 4.0 seconds. 
hide_delay = 1.0

# this parameter defines the app indicator type
# valid types are dots or lines 
# default is dots
indicator_type = lines

[separator]
# the separator can be set with or without line
# true show the separator line otherwise set it to false
# default true
show_line = true

# set the vertical or horizontal separator size
# default 8
separator_size = 0

# this parameter defines the default separator distance between icons.
# default value is 12. min value is 1 max value 30 pixels.
separator_margin = 16


